{
	"name": "web",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "next build",
		"start": "next start",
		"dev": "next",
		"test": "jest",
		"lint": "xo",
		"test:watch": "jest --watch",
		"generate": "gql-gen --config codegen.yml"
	},
	"dependencies": {
		"@chakra-ui/core": "^0.8.0",
		"@emotion/core": "^10.0.28",
		"@emotion/styled": "^10.0.27",
		"@sanity/block-content-to-react": "^2.0.7",
		"@sanity/image-url": "^0.140.17",
		"axios": "^0.21.0",
		"date-fns": "^2.15.0",
		"emotion-theming": "^10.0.27",
		"get-youtube-id": "^1.0.1",
		"graphql": "^15.0.0",
		"graphql-tag": "^2.10.3",
		"next": "^9.5.1",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-icons": "^3.10.0",
		"react-youtube": "^7.11.3",
		"uuid": "^8.3.0"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^1.17.7",
		"@graphql-codegen/schema-ast": "^1.13.3",
		"@graphql-codegen/typescript": "^1.17.7",
		"@graphql-codegen/typescript-operations": "^1.17.7",
		"@testing-library/jest-dom": "^5.11.2",
		"@testing-library/react": "^11.0.4",
		"@types/jest": "^26.0.8",
		"@types/node": "^14.0.27",
		"@types/react": "^16.9.2",
		"@types/react-dom": "^16.9.1",
		"@types/uuid": "^8.0.0",
		"babel-jest": "^26.2.2",
		"jest": "^26.2.2",
		"typescript": "^4.0.3",
		"vercel": "^20.1.1"
	},
	"babel": {
		"plugins": [],
		"presets": [
			"next/babel"
		]
	},
	"jest": {
		"collectCoverageFrom": [
			"**/*.{js,jsx,ts,tsx}",
			"!**/*.d.ts",
			"!**/node_modules/**"
		],
		"setupFilesAfterEnv": [
			"@testing-library/jest-dom/extend-expect"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/.next/"
		],
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
		},
		"transformIgnorePatterns": [
			"/node_modules/"
		]
	}
}
