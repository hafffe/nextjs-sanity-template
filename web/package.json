{
	"name": "web",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "next build",
		"start": "next start",
		"dev": "next",
		"test": "jest",
		"lint": "xo",
		"test:watch": "jest --watch",
		"typecheck": "tsc",
		"generate": "gql-gen --config codegen.yml"
	},
	"dependencies": {
		"@chakra-ui/react": "^1.0.1",
		"@chakra-ui/theme-tools": "^1.0.1",
		"@emotion/react": "^11.1.1",
		"@emotion/styled": "^11.0.0",
		"@sanity/block-content-to-react": "^2.0.7",
		"@sanity/image-url": "^0.140.19",
		"axios": "^0.21.0",
		"date-fns": "^2.16.1",
		"framer-motion": "^2.9.4",
		"get-youtube-id": "^1.0.1",
		"graphql": "^15.4.0",
		"graphql-tag": "^2.11.0",
		"next": "^10.0.0",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-icons": "^4.1.0",
		"react-youtube": "^7.13.0",
		"uuid": "^8.3.1"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^1.19.3",
		"@graphql-codegen/schema-ast": "^1.18.1",
		"@graphql-codegen/typescript": "^1.18.1",
		"@graphql-codegen/typescript-operations": "^1.17.12",
		"@testing-library/jest-dom": "^5.11.6",
		"@testing-library/react": "^11.2.2",
		"@types/jest": "^26.0.15",
		"@types/node": "^14.14.10",
		"@types/react": "^16.9.2",
		"@types/react-dom": "^16.9.1",
		"@types/uuid": "^8.3.0",
		"babel-jest": "^26.6.3",
		"jest": "^26.6.3",
		"typescript": "^4.1.2",
		"vercel": "^21.0.1"
	},
	"babel": {
		"plugins": [],
		"presets": [
			"next/babel"
		]
	},
	"jest": {
		"collectCoverageFrom": [
			"**/*.{js,jsx,ts,tsx}",
			"!**/*.d.ts",
			"!**/node_modules/**"
		],
		"setupFilesAfterEnv": [
			"@testing-library/jest-dom/extend-expect"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/.next/"
		],
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
		},
		"transformIgnorePatterns": [
			"/node_modules/"
		]
	}
}
